{
  "ConnectionStrings": {
    "SGIContextSQL": "Data Source=.\\SQLEXPRESS;Initial Catalog=Base;Trusted_Connection=false;User ID=admin;Password=admin;"
  },
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          //"path": "Log\\Log-{Date}.txt",
          "pathFormat": "Log\\Log-{Date}.txt",
          "outputTemplate": "{Timestamp:dd/MM/yyyyy HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "Web API"
    }
  },
  "Jwt": {
    "SecretKey": "THIS_IS_SECRET_KEY"
  }
}
